<div id="conocenos" class="row py-4">
    <div class="col-md-4 offset-md-2">
        <h2>visitanos</h2>
        <div class="embed-responsive embed-responsive-16by9">
            <iframe class="embed-responsive-item"
                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3277.631600900327!2d-58.611544448970065!3d-34.764871880322!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x95bcc4f9acdd9a15%3A0x8cae0283d4923a5f!2sJuan%20Zufriategui%205747%2C%20B1758ALI%20Gonz%C3%A1lez%20Cat%C3%A1n%2C%20Provincia%20de%20Buenos%20Aires!5e0!3m2!1ses!2sar!4v1655307250498!5m2!1ses!2sar"
                style="border:0;" allowfullscreen></iframe>

        </div>
    </div>
    <div class="col-md-4 mt-4 mt-sm-0">
        <h2>contactanos</h2>
        <form action="/" method="post">
            <div class="form-row">
                <div class="form-group col-md-6">
                    <label for="nombre">nombre</label>
                    <input type="text" class="form-control" id="nombre" name="nombre">

                </div>
                <div class="form-group col-md-6">
                    <label for="inputPassword4">apellido</label>
                    <input type="text" class="form-control" id="apellido" name="apellido">
                </div>
            </div>
            <div class="form-group">
                <label for="email">Email</label>
                <input type="email" class="form-control" id="email" placeholder="" name="email">
            </div>
            <div class="form-group">
                <label for="tel">Telefono</label>
                <input type="text" class="form-control" id="tel" placeholder="" name="tel">
            </div>
            <div class="form-grop">
                <label for="mensaje">Menaje</label>
                <textarea class="form-control" id="mensaje" rows="4" name="mensaje"></textarea>
            </div>
            <button type="submit" class="btn btn-primary">Enviar</button>
        </form>
        {{#if message}}
        <p class="mensaje_enviado">{{message}} </p>
        {{/if}}
    </div>
</div>

